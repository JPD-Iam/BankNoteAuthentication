    "paths": {
      "/predict": {
        "get": {
          "summary": "Authenticate the Bank Note",
          "parameters": [
            {
              "name": "variance",
              "in": "query",
              "required": true,
              "schema": {
                "type": "number"
              }
            },
            {
              "name": "skewness",
              "in": "query",
              "required": true,
              "schema": {
                "type": "number"
              }
            },
            {
              "name": "curtosis",
              "in": "query",
              "required": true,
              "schema": {
                "type": "number"
              }
            },
            {
              "name": "entropy",
              "in": "query",
              "required": true,
              "schema": {
                "type": "number"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "The output values",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "prediction": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            },
            "400": {
              "description": "Invalid input"
            }
          }
        }
      }
    }
  }
  